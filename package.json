{
  "name": "thehaguetech-site",
  "version": "0.1.0",
  "description": "The Hague Tech on the Web",
  "main": "index.js",
  "scripts": {
    "dev-without-server": "next",
    "dev": "export $(cat .env | xargs) && node api/server.js",
    "start": "node ./node_modules/next/dist/bin/next build && NODE_ENV=production node api/server.js",
    "build": "next build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "next build && next export"
  },
  "plugins": [
    [
      "styled-jsx/babel",
      {
        "optimizeForSpeed": true
      }
    ]
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thehaguetech/thehaguetech-site.git"
  },
  "keywords": [
    "thehaguetech"
  ],
  "author": "bartwr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thehaguetech/thehaguetech-site/issues"
  },
  "homepage": "https://github.com/thehaguetech/thehaguetech-site#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "chalk": "^1.1.3",
    "cli-table2": "^0.2.0",
    "contentful": "^7.7.0",
    "express": "^4.17.1",
    "google-map-react": "^1.1.4",
    "jquery": "^3.4.1",
    "mailchimp-api-v3": "^1.13.0",
    "marked": "^0.6.2",
    "moment": "^2.24.0",
    "next": "^8.1.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-swipe": "^6.0.4",
    "sitemap": "^2.2.0",
    "styled-jsx": "^3.2.1",
    "swipe-js-iso": "^2.1.5"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
